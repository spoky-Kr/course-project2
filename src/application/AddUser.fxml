<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddUserController">
   <children>
      <Pane prefHeight="200.0" prefWidth="200.0">
         <children>
            <PasswordField layoutX="23.0" layoutY="81.0" promptText="Пароль" />
            <TextField layoutX="26.0" layoutY="39.0" promptText="Логин" />
         </children>
      </Pane>
      <ButtonBar layoutY="191.0" prefHeight="40.0" prefWidth="216.0">
        <buttons>
          <Button fx:id="OK" mnemonicParsing="false" onAction="#onOk" text="ОК" />
            <Button fx:id="OTMENA" mnemonicParsing="false" onAction="#OnOTMENA" text="Отмена" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
